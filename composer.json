{
    "name": "quadstudio/laravel-tools",
    "description": "Language tools for Laravel 5",
    "keywords": ["laravel", "language", "tools"],
    "homepage": "https://github.com/quadstudio/laravel-tools",
    "type": "library",
    "license": "MIT",
    "version": "0.1.1",
    "authors": [
        {
            "name": "Alexander Shuleyko",
            "email": "salex009@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "QuadStudio\\Tools\\": "src/Tools/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
      "php": ">=7.1.3",
      "illuminate/support": "5.6.*",
      "illuminate/console": "5.6.x",
      "illuminate/view": "5.6.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "QuadStudio\\Tools\\ToolsServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
